!@UTM-PH
config t
 ip host bet11.com 208.8.8.11
 service finger
 service tcp-small-servers
 service udp-small-servers
 ip dns server
 ip http server
 ip http secure-server
 telephony-service
  no auto-reg-ephone
  max-ephones 5
  max-dn 20
  ip source-address 208.8.8.11 port 2000
  exit
 voice service voip
  allow-connections h323 to sip
          
  allow-connections sip to h323
  allow-connections sip to sip
  supplementary-service h450.12
 sip
   bind control source-interface g4
   bind media source-interface g4
   registrar server expires max 600 min 60
 voice register global
  mode cme
  source-address 208.8.8.11 port 5060
  max-dn 12
  max-pool 12
  authenticate register
  create profile sync syncinfo.xml
  end

task 4 - bet 11.com : permit : ssh, ping ,sip, sccp, dns

config t
 no ip access-list extended FWP10
 ip access-list extended FWP10
 permit tcp any host bet11.com eq 22 log
  permit icmp any host bet11.com log
permit tcp any host bet11.com eq 5060 log
permit tcp any host bet11.com eq 53 log
permit tcp any host bet11.com eq 2000 log
  exit
 !
 int gi 1
  no ip access-group FWP10 in
  end
show ip access-list int g1

task 5 - bet11.com : open the ff ports: telnet, ftp, smtp, mysql, http, https

config t
 no ip access-list extended FWP10
 ip access-list extended FWP10
 permit tcp any host bet11.com eq 23 log
  permit icmp any host bet11.com log
permit tcp any host bet11.com eq 21 log
permit tcp any host bet11.com eq 25 log
permit tcp any host bet11.com eq 3306 log
permit tcp any host bet11.com eq 80 log
permit tcp any host bet11.com eq 433 log
  exit
!
int gi 1
 ip access-group FWP10 in
  end
show ip access-list int g1

task 6 Create an extended ACL for `www.bet11.com` that will open the following ports
SIP, SSH, HTTPS, DNS, SUBMISSION, IMAPS, POP3S, SMTPS, SNMP

sh run | s int
 
config t
 no ip access-list extended FWP10
 ip access-list extended FWP10
 
permit tcp any host bet11.com eq 5060 log
permit tcp any host bet11.com eq 22 log
permit tcp any host bet11.com eq 433 log
permit tcp any host bet11.com eq 53 log
permit tcp any host bet11.com eq 587 log
permit tcp any host bet11.com eq 993 log
permit tcp any host bet11.com eq 465 log
permit tcp any host bet11.com eq 995 log
permit tcp any host bet11.com eq 162 log
  exit
!
int gi 1
 no ip access-group FWP10 in
  end
show ip access-list int g1


